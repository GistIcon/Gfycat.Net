{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj",
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
          ],
          "src": ".."
        }
      ],
      "dest": "obj/api",
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "obj/api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
      }
    ],
    "resource": [{ "files": [ "images/**" ]}],
    "overwrite": [{ "files": [ "apidoc/**.md" ]}],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata":{
      "_enableSearch":true
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
